{
  "name": "@wistudent/jest-each-improved-types",
  "version": "2.0.4",
  "description": "Using recursive conditional types from typescript 4.1 to improve typings of table driven jest-each tests.",
  "author": "Tobias Trumm",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.4.1",
    "@jest/types": "^29.4.1",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.4.1",
    "jest": "^29.4.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@jest/globals": ">=29.4.1",
    "@jest/types": "^29.4.1",
    "typescript": ">=4.1.x"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WIStudent/jest-each-improved-types.git"
  }
}
